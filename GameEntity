public class GameEntity {
    
    private int pixelNumber;
    private String mainColor;
    private String secondColor;
    
    public GameEntity() {
        this.pixelNumber = 50;
        this.mainColor = "Blue";
        this.secondColor = "White";
  
    }
    
    public int getPixel () {
        return this.pixelNumber;
    }
    
    public void setPixel (int pixelNum) {
        this.pixelNumber = pixelNum;
    }
    
    
    
    public String draw() {
        return "Drawdrawdrawdraw";
    }
}
